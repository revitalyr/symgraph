{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug executable 'symgraph-cli'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "run",
                    "--bin=symgraph-cli",
                    "--package=symgraph-cli"
                ]
            },
            "cwd": "${workspaceFolder}/crates/symgraph-cli",
            "args": ["scan-rust", "--manifest-path", ".", "--lsif", "project.lsif", "--db", "mydatabase.db"]
        },
        {
            "name": "Debug orthosis_mirror_app scan",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/symgraph-cli.exe",
            "args": ["scan-rust", "--manifest-path", "D:\\work\\Projects\\STL Desktop Tool\\orthosis_mirror_app", "--db", "D:\\work\\Projects\\STL Desktop Tool\\orthosis_mirror_app\\orthosis_mirror_app.db"],
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo build symgraph-cli"
        },
        {
            "name": "Debug unit tests in executable 'symgraph-cli'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--bin=symgraph-cli",
                    "--package=symgraph-cli"
                ]
            }
        },
        {
            "name": "Debug integration test 'integration'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--test=integration",
                    "--package=symgraph-cli"
                ]
            }
        },
        {
            "name": "Debug unit tests in library 'symgraph_core'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--package=symgraph-core"
                ]
            }
        },
        {
            "name": "Debug unit tests in library 'symgraph_cxx'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--package=symgraph-cxx"
                ]
            }
        },
        {
            "name": "Debug unit tests in library 'symgraph_models'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--package=symgraph-models"
                ]
            }
        },
        {
            "name": "Debug unit tests in library 'symgraph_discovery'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--package=symgraph-discovery"
                ]
            }
        },
        {
            "name": "Debug unit tests in library 'symgraph_rust'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--package=symgraph-rust"
                ]
            }
        }
    ]
}