{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cargo build",
			"type": "shell",
			"command": "cargo",
			"args": ["build"],
			"isBackground": false,
			"problemMatcher": ["$rustc"],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "cargo check",
			"type": "shell",
			"command": "cargo",
			"args": ["check"],
			"isBackground": false,
			"problemMatcher": ["$rustc"],
			"group": "build"
		}
	]
}